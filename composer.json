{
    "name": "ekreative/query-parameter-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["annotations","query","query-parameter"],
    "authors": [
        {
            "name": "Alex Moshta",
            "email": "ahonyous@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ekreative\\QueryParameterBundle\\": "src/Ekreative/QueryParameterBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Ekreative\\QueryParameterBundle\\": "tests/Ekreative/QueryParameterBundle/" },
        "classmap": ["tests/TestKernel.php"]
    },
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "^3.0|^4.0",
        "sensio/framework-extra-bundle": "^3.0|^4.0|^5.0",
        "symfony/validator": "^3.0|^4.2",
        "symfony/form": "^3.0|^4.2"
    },
    "require-dev": {
        "symfony/browser-kit": "^3.0|^4.0",
        "phpunit/phpunit": "^7.0",
        "symfony/monolog-bundle": "^3.0|^4.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "symfony/yaml": "^3.0|^4.0"
    },
    "extra": {
        "symfony": {
            "require": "^4.2"
        }
    }
}
